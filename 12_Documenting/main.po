#: main.c:74
#, c-format
msgid ""
"OVERVIEW: number guesser\n"
"\n"
msgstr ""
"ОБЗОР: угадыватель чисел\n"
"\n"

#: main.c:75
#, c-format
msgid ""
"USAGE: main [options] \n"
"\n"
msgstr ""
"ИСПОЛЬЗОВАНИЕ: main [параметры] \n"
"\n"

#: main.c:76
#, c-format
msgid ""
"OPTIONS:\n"
"\t-r\tPrint numbers in roman numerals\n"
msgstr ""
"ПАРАМЕТРЫ:\n"
"\t-r\tВывод числа в римской системе счисления\n"

#: main.c:99
#, c-format
msgid "Please pick a random number from 1 to 100\n"
msgstr "Пожалуйста, загадайте число от 1 до 100\n"

#: main.c:100
#, c-format
msgid "Let's get started\n"
msgstr "Давайте начнем!\n"

#: main.c:105
#, c-format
msgid "Is your number larger than %s?\n"
msgstr "Загаданное число больше чем %s?\n"

#: main.c:107
#, c-format
msgid "Is your number larger than %d?\n"
msgstr "Загаданное число больше чем %d?\n"

#: main.c:112
#, c-format
msgid ""
"Input went very wrong\n"
"Please, enter y or n\n"
msgstr ""
"Что-то сломалось\n"
"Пожалуйста, введите д или н\n"

#: main.c:116
msgid "y"
msgstr "д"

#: main.c:118
msgid "n"
msgstr "н"

#: main.c:121
#, c-format
msgid "Please, enter y or n\n"
msgstr "Пожалуйста, введите д или н\n"

#: main.c:126
#, c-format
msgid "You have picked number %s!\n"
msgstr "Вы загадали число %s!\n"

#: main.c:128
#, c-format
msgid "You have picked number %d!\n"
msgstr "Вы загадали число %d!\n"
